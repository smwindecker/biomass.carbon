  year = {2014},
  keywords = {determinants of plant community diversity and structure,functional traits,geographic and climatic distributions,Kolmogorovâ€“Smirnov Importance index,leaf nitrogen,leaf size,maximum adult height,phylogenetic tree,seed mass,specific leaf area},
